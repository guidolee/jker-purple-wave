	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#7F7F7F",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, storage.type.js, storage.type.function.js, variable.other.flowtype.js, storage.type.extends.js, storage.type.function.js, storage.type.class.js",
			"settings": {
				"foreground": "#858585"
			}
		},
		{
			"name": "Function Call",
			"scope": "variable.function.js, meta.class meta.group.braces.curly meta.function-call variable.function.js, entity.name.tag.jsx, variable.function.constructor.js",
			"settings": {
				"foreground": "#ffe2a9"
			}
		},
		{
			"name": "Function/Class Name",
			"scope": "entity.name.function.js, meta.class.js entity.name.class, meta.class meta.function-call variable.function.js, source.js keyword.control.loop.js, storage.type.function.arrow.js",
			"settings": {
				"foreground": "#a5f1ce"
			}
		},
		{
			"name": "Control Flow",
			"scope": "keyword.control.flow.js, keyword.control.trycatch.js, source.js meta.group.braces.curly.js keyword.control.loop.js, keyword.control.switch.js",
			"settings": {
				"foreground": "#f1a5ab"
			}
		},
		{
			"name": "Assignment",
			"scope": "keyword.operator.assignment",
			"settings": {
				"foreground": "#ffe2a9"
			}
		},
		{
			"name": "Logic",
			"scope": "keyword.operator.logical.js",
			"settings": {
				"foreground": "#ffe2a9"
			}
		},
		{
			"name": "Comparison",
			"scope": "keyword.operator.comparison.js, keyword.operator.relational.js",
			"settings": {
				"foreground": "#f1a5ab"
			}
		},
		{
			"name": "String",
			"scope": "string.quoted, string.interpolated",
			"settings": {
				"foreground": "#a9cfa4"
			}
		},
		{
			"name": "Literal",
			"scope": "constant.language, constant.numeric, support.constant, constant.character, constant.other.color, constant.other.symbol, constant.other.key, keyword.other.unit",
			"settings": {
				"foreground": "#dbbbeb"
			}
		},
		{
			"name": "URL",
			"scope": "*url*, *link*, *uri*",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Search Results Lines",
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#99C794"
			}
		},
		{
			"scope": "source.js entity.other.attribute-name.js",
			"settings": {
				"foreground": "#eeeeee",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "text.html.basic entity.other.attribute-name.html",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "HTML element",
			"scope": "entity.name.tag.html",
			"settings": {
				"foreground": "#cf7981"
			}
		},
		{
			"name": "JSX element",
			"scope": "entity.name.tag.js",
			"settings": {
				"foreground": "#c3f0be"
			}
		},
		{
			"name": "CSS ID",
			"scope": "entity.other.attribute-name.id",
			"settings": {
				"foreground": "#80aa7b"
			}
		},
		{
			"name": "CSS class",
			"scope": "entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#a9cfa4"
			}
		},
		{
			"name": "CSS class",
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#cf7981"
			}
		},
		{
			"name": "CSS pseudo-class, pseudo-element",
			"scope": "entity.other.attribute-name.pseudo-class.css, entity.other.attribute-name.pseudo-element.css",
			"settings": {
				"foreground": "#c0e7bb"
			}
		},
		{
			"name": "CSS attr name",
			"scope": "entity.other.attribute-name.attribute.css",
			"settings": {
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "CSS property constant",
			"scope": "constant.numeric.css, support.constant.property-value.css",
			"settings": {
				"foreground": "#f3c7a4"
			}
		},
		{
			"name": "CSS property value",
			"scope": "keyword.other.unit, meta.property-value.css, keyword.other.important.css",
			"settings": {
				"foreground": "#e4ad80"
			}
		},
		{
			"name": "LESS/SASS Variable",
			"scope": "variable.other.less, variable.other.scss",
			"settings": {
				"foreground": "#9ae0dd"
			}
		}
	]
}